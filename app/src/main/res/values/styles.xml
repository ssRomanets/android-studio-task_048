        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/MarginLeftNormal</item>
        <item name="android:layout_marginRight">@dimen/MarginRightNormal</item>
        <item name="android:background">@color/textViewColor</item>
        <item name="android:textColor">@color/textViewTextColor</item>
        <item name="android:textSize">@dimen/textSizeNormal</item>
