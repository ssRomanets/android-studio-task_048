        <item name="android:layout_marginRight">@dimen/MarginRightLarge</item>
        <item name="android:background">@color/textViewColor</item>
        <item name="android:textColor">@color/textViewTextColor</item>
        <item name="android:textSize">@dimen/textSizeLarge</item>
    </style>

    <style name="LLHNormalStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/MarginTopNormal</item>
        <item name="android:orientation">horizontal</item>
    </style>

