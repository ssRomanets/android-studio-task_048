<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TextViewMainStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/MarginTopLarge</item>
        <item name="android:layout_marginBottom">@dimen/MarginBottomLarge</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@color/textViewColor</item>
        <item name="android:textColor">@color/textViewTextColor</item>
        <item name="android:textSize">@dimen/textSizeLarge</item>
    </style>

    <style name="TextViewSubMainStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/MarginTopNormal</item>
        <item name="android:layout_marginBottom">@dimen/MarginBottomNormal</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:background">@color/textViewColor</item>
        <item name="android:textColor">@color/textViewTextColor</item>
        <item name="android:textSize">@dimen/textSizeNormal</item>
    </style>

    <style name="LLHLargeStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/MarginTopLarge</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="TextViewLargeStyle">
        <item name="android:layout_width">wrap_content</item>
